comp-lex-item := raise-sem-lex-item & non-local-none-lex-item & non-mod-lex-item &
  [ SYNSEM.LOCAL.CAT [ HEAD comp,
		       VAL [ SUBJ < >,
			     SPR < >,
           SPEC < >,
			     COMPS < #comps &
				     [ LOCAL.CAT [ HEAD verb,
						   VAL.SUBJ < > ]] > ]],
    ARG-ST < #comps > ].

prop-comp-lex-item := comp-lex-item &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.SF prop ].

