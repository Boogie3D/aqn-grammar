  [ C-CONT [ RELS.LIST < arg1-ev-relation &
                         [ LBL #ltop,
                           ARG1 #index ] >,
             HOOK #hook ],
    DTR.SYNSEM.LOCAL [ CAT.HEAD noun,
                       CONT.HOOK #hook &
                                 [ LTOP #ltop,
                                   INDEX #index ] ] ].

