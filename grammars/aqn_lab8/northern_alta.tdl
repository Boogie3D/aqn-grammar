                      LOCATIVE-VERB-FLAG #locative-verb,
                      MEN_INTRANS-VERB-FLAG #men_intrans-verb,
                      SUFFIX-FLAG #suffix,
                      INFIX_UM_SECOND-FLAG #infix_um_second,
