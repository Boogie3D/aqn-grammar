infix_um_second-rule-dtr := word-or-lexrule.
infix_perfect_third-rule-dtr := word-or-lexrule.

;;; Lexical rule types

prefixes-lex-rule-super := add-only-no-ccont-rule & initial_suffix-rule-dtr & ultimate_suffix-rule-dtr &
  [ INFLECTED [ PREFIXES-FLAG +,
                ULTIMATE_SUFFIX-FLAG #ultimate_suffix,
                UM-VERB-FLAG #um-verb,
                INITIAL_SUFFIX-FLAG #initial_suffix,
                PATIENT-VERB-FLAG #patient-verb,
                CONVEYANCE-VERB-OR-MEN_TRANS-VERB-OR-STATIVE-VERB-OR-STATIVE_INTRANSITIVE-VERB-FLAG #conveyance-verb-or-men_trans-verb-or-stative-verb-or-stative_intransitive-verb,
                CONVEYANCE-VERB-FLAG #conveyance-verb,
                LOCATIVE-VERB-OR-PATIENT-VERB-FLAG #locative-verb-or-patient-verb,
                NOT_PROGRESSIVE-FLAG #not_progressive,
                STATIVE-VERB-OR-STATIVE_INTRANSITIVE-VERB-FLAG #stative-verb-or-stative_intransitive-verb,
                INTRANS_VERBS-VERB-FLAG #intrans_verbs-verb,
                PROGRESSIVE_FIRST_INFIX-FLAG #progressive_first_infix,
                LOCATIVE-VERB-FLAG #locative-verb,
                MEN_INTRANS-VERB-FLAG #men_intrans-verb,
                SUFFIX-FLAG #suffix,
                INFIX_UM_SECOND-FLAG #infix_um_second,
                PREFIX_VOICE_FINAL-FLAG #prefix_voice_final,
                INFIX_PERFECT_THIRD-FLAG #infix_perfect_third,
                MEN_TRANS-VERB-FLAG #men_trans-verb,
                NONINFLECTED_TRANS-VERB-FLAG #noninflected_trans-verb],
    DTR unvoiced_common-noun-lex &
        [ INFLECTED [ ULTIMATE_SUFFIX-FLAG #ultimate_suffix,
                      UM-VERB-FLAG #um-verb,
                      INITIAL_SUFFIX-FLAG #initial_suffix,
                      PATIENT-VERB-FLAG #patient-verb,
                      CONVEYANCE-VERB-OR-MEN_TRANS-VERB-OR-STATIVE-VERB-OR-STATIVE_INTRANSITIVE-VERB-FLAG #conveyance-verb-or-men_trans-verb-or-stative-verb-or-stative_intransitive-verb,
                      CONVEYANCE-VERB-FLAG #conveyance-verb,
                      LOCATIVE-VERB-OR-PATIENT-VERB-FLAG #locative-verb-or-patient-verb,
                      NOT_PROGRESSIVE-FLAG #not_progressive,
                      STATIVE-VERB-OR-STATIVE_INTRANSITIVE-VERB-FLAG #stative-verb-or-stative_intransitive-verb,
                      INTRANS_VERBS-VERB-FLAG #intrans_verbs-verb,
                      PROGRESSIVE_FIRST_INFIX-FLAG #progressive_first_infix,
                      LOCATIVE-VERB-FLAG #locative-verb,
                      MEN_INTRANS-VERB-FLAG #men_intrans-verb,
                      SUFFIX-FLAG #suffix,
                      INFIX_UM_SECOND-FLAG #infix_um_second,
                      PREFIX_VOICE_FINAL-FLAG #prefix_voice_final,
                      INFIX_PERFECT_THIRD-FLAG #infix_perfect_third,
                      MEN_TRANS-VERB-FLAG #men_trans-verb,
                      NONINFLECTED_TRANS-VERB-FLAG #noninflected_trans-verb] ] ].

